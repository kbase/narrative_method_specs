{
  "name" : "Normalize Abundance Profile",
  "ver" : "1.0.0",
  "authors" : [ ],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : ["active"],
  "widgets" : {
    "input" : null,
    "output" : "AbundanceDataTable"
  },
  "parameters" : [ {
    "id" : "input_profile",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "Communities.TaxonomicMatrix", "Communities.FunctionalMatrix" ]
    }
  }, {
    "id" : "output_profile",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "Communities.TaxonomicMatrix", "Communities.FunctionalMatrix" ],
      "is_output_name": true
    }
  } ],
  "behavior" : {
    "script-mapping" : {
        "module" : "communities_api",
        "script" : "mg-compare-normalize",
        "has_files": true,
        "input_mapping" : [
            {
                "input_parameter": "input_profile",
                "target_property": "input"
            },
            {
                "input_parameter": "output_profile",
                "target_property": "output"
            }
        ],
        "output_mapping" : [
            {
                "input_parameter": "output_profile",
                "target_property": "id"
            },
            {
                "narrative_system_variable": "workspace",
                "target_property": "ws"
            },
            {
                "narrative_system_variable": "token",
                "target_property": "auth"
            }
        ]
    }
  }
}
