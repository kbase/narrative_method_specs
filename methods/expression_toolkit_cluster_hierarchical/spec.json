{
  "name" : "Expression Toolkit cluster hierarchical",
  "ver" : "1.0.0",
  "authors" : [ ],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : ["active", "expression"],
  "widgets" : {
    "input" : null,
    "output" : "kbaseExpressionFeatureClusters"
  },
  "parameters" : [ {
    "id" : "input_expression_matrix",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "KBaseFeatureValues.ExpressionMatrix" ]
    }
  },{
    "id" : "feature_height_cutoff",
    "optional" : true,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "0.5" ],
    "field_type" : "text",
    "text_options" : {
      "validate_as": "float",
      "min_int" : 0.0,
      "max_int" : 1.0
    }
  }, {
    "id" : "output_cluster_set",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "KBaseFeatureValues.FeatureClusters" ],
      "is_output_name":true
    }
  }, {
    "id" : "max_items",
    "optional" : true,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "5000" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ ],
      "validate_as": "int",
      "min_int" : 2
    }
  }, {
    "id" : "algorithm",
    "optional" : true,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "flashClust" ],
    "field_type" : "dropdown",
    "dropdown_options":{
      "options": [
        {
          "value": "hclust",
          "display": "hclust",
          "id": "hclust",
          "ui_name": "hclust"
        },
        {
          "value": "flashClust",
          "display": "flashClust",
          "id": "flashClust",
          "ui_name": "flashClust"
        }
      ]
    }
  } ],
  "behavior" : {
    "service-mapping" : {
      "url" : "https://kbase.us/services/feature_values/jsonrpc",
      "name" : "KBaseFeatureValues",
      "method" : "cluster_hierarchical",
      "input_mapping" : [
        {
          "input_parameter": "feature_height_cutoff",
          "target_property": "feature_height_cutoff"
        },
        {
          "input_parameter": "input_expression_matrix",
          "target_property": "input_data",
          "target_type_transform": "ref"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "out_workspace"
        },
        {
          "input_parameter": "output_cluster_set",
          "target_property": "out_clusterset_id"
        },
        {
          "input_parameter": "max_items",
          "target_property": "max_items"
        },
        {
          "input_parameter": "algorithm",
          "target_property": "algorithm"
        }
      ],
      "output_mapping" : [
        {
          "input_parameter": "output_cluster_set",
          "target_property": "clusterSetID"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspaceID"
        },
        {
          "service_method_output_path": [],
          "target_property": "job_id"
        }
      ]
    }
  },
  "job_id_output_field": "job_id"
}

