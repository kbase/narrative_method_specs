{
  "name" : "Generate Heatmap from Abundance Data",
  "ver" : "1.0.0",
  "authors" : [ ],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : ["inactive"],
  "widgets" : {
    "input" : null,
    "output" : "AbundanceDataHeatmap"
  },
  "parameters" : [ {
    "id" : "input_profile",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "Communities.TaxonomicMatrix", "Communities.FunctionalMatrix" ]
    }
  }, {
    "id" : "output_heatmap",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "Communities.Heatmap" ],
      "is_output_name": true
    }
  }, {
    "id" : "distance",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "euclidean" ],
    "field_type" : "dropdown",
    "dropdown_options" : {
        "options": [
        {
            "value" : "euclidean",
            "display" : "euclidean"
        },
        {
            "value" : "bray-curtis",
            "display" : "bray-curtis"
        },
        {
            "value" : "maximum",
            "display" : "maximum"
        },
        {
            "value" : "manhattan",
            "display" : "manhattan"
        },
        {
            "value" : "canberra",
            "display" : "canberra"
        },
        {
            "value" : "minkowski",
            "display" : "minkowski"
        },
        {
            "value" : "difference",
            "display" : "difference"
        }
    ] }
  }, {
    "id" : "cluster",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "ward" ],
    "field_type" : "dropdown",
    "dropdown_options" : {
        "options": [
        {
            "value" : "ward",
            "display" : "ward"
        },
        {
            "value" : "single",
            "display" : "single"
        },
        {
            "value" : "complete",
            "display" : "complete"
        },
        {
            "value" : "mcquitty",
            "display" : "mcquitty"
        },
        {
            "value" : "median",
            "display" : "median"
        },
        {
            "value" : "centroid",
            "display" : "centroid"
        }
    ] }
  }, {
    "id" : "name",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "0" ],
    "field_type" : "checkbox",
    "checkbox_options": {
        "checked_value": 1,
        "unchecked_value": 0
    }
  } ],
  "behavior" : {
        "script-mapping" : {
            "module" : "communities_api",
            "script" : "mg-compare-heatmap",
            "has_files": true,
            "input_mapping" : [
                {
                    "input_parameter": "input_profile",
                    "target_property": "input"
                },
                {
                    "input_parameter": "output_heatmap",
                    "target_property": "output"
                },
                {
                    "input_parameter": "distance",
                    "target_property": "distance"
                },
                {
                    "input_parameter": "cluster",
                    "target_property": "cluster"
                },
                {
                    "input_parameter": "name",
                    "target_property": "name"
                }
            ],
            "output_mapping" : [
                {
                    "input_parameter": "output_heatmap",
                    "target_property": "id"
                },
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "ws"
                },
                {
                    "narrative_system_variable": "token",
                    "target_property": "auth"
                }
            ]
        }
  }
}
