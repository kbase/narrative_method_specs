{
  "name" : "Propagate model to new genome",
  "ver" : "1.0.0",
  "authors" : [ ],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : ["active"],
  "widgets" : {
    "input" : null,
    "output" : "kbaseTabTable"
  },
  "parameters" : [ {
    "id" : "input_model",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "KBaseFBA.FBAModel" ]
    }
  }, {
    "id" : "input_protcomp",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "GenomeComparison.ProteomeComparison" ]
    }
  }, {
    "id" : "output_model",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "KBaseFBA.FBAModel" ],
      "is_output_name" : true
    }
  }, {
    "id": "keep_nogene_rxn",
    "optional":false,
    "advanced":true,
    "allow_multiple":false,
    "default_values":["1"],
    "field_type" : "checkbox",
    "checkbox_options":{
      "checked_value": 1,
      "unchecked_value": 0
    }
  } ],
  "behavior" : {
    "service-mapping" : {
      "url" : "",
      "name" : "KBaseFBAModeling",
      "method" : "translate_fbamodel",
      "input_mapping" : [
        {
          "input_parameter": "input_protcomp",
          "target_property": "protcomp"
        },
        {
          "input_parameter": "input_model",
          "target_property": "model"
        },
        {
          "input_parameter": "output_model",
          "target_property": "output_id"
        },
        {
          "input_parameter": "keep_nogene_rxn",
          "target_property": "keep_nogene_rxn"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "workspace"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "protcomp_workspace"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "model_workspace"
        }
      ],
      "output_mapping" : [
        {
          "constant_value": "KBaseFBA.FBAModel",
          "target_property": "type"
        },
        {
          "input_parameter": "output_model",
          "target_property": "obj"
        },
        {
          "narrative_system_variable": "workspace",
          "target_property": "ws"
        }
      ]
    }
  }
}
