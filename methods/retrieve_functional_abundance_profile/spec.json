{
  "name" : "Generate Functional Abundance Data",
  "ver" : "1.0.0",
  "authors" : [ ],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : ["inactive"],
  "widgets" : {
    "input" : null,
    "output" : "AbundanceDataView"
  },
  "parameters" : [ {
    "id" : "input_metagenomes",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "Communities.Collection", "Communities.MetagenomeSet" ]
    }
  }, {
    "id" : "output_profile",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "Communities.FunctionalMatrix" ],
      "is_output_name": true
    }
  }, {
    "id" : "level",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "level3" ],
    "field_type" : "dropdown",
    "dropdown_options" : {
        "options": [
        {
            "value" : "function",
            "display" : "function"
        },
        {
            "value" : "level3",
            "display" : "level3"
        },
        {
            "value" : "level2",
            "display" : "level2"
        },
        {
            "value" : "level1",
            "display" : "level1"
        }
    ] }
  }, {
    "id" : "source",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "Subsystems" ],
    "field_type" : "dropdown",
    "dropdown_options" : {
        "options": [
        {
            "value" : "Subsystems",
            "display" : "Subsystems"
        },
        {
            "value" : "NOG",
            "display" : "NOG"
        },
        {
            "value" : "COG",
            "display" : "COG"
        },
        {
            "value" : "KO",
            "display" : "KO"
        }
    ] }
  }, {
    "id" : "evalue",
    "optional" : false,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "5" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ ]
    }
  }, {
    "id" : "identity",
    "optional" : false,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "60" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ ]
    }
  }, {
    "id" : "length",
    "optional" : false,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "15" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ ]
    }
  } ],
  "behavior" : {
      "script-mapping" : {
          "module" : "communities_api",
          "script" : "mg-compare-functions",
          "has_files": true,
          "input_mapping" : [
              {
                  "input_parameter": "input_metagenomes",
                  "target_property": "ids"
              },
              {
                  "input_parameter": "output_profile",
                  "target_property": "output"
              },
              {
                  "input_parameter": "level",
                  "target_property": "level"
              },
              {
                  "input_parameter": "source",
                  "target_property": "source"
              },
              {
                  "input_parameter": "evalue",
                  "target_property": "evalue"
              },
              {
                  "input_parameter": "identity",
                  "target_property": "identity"
              },
              {
                  "input_parameter": "length",
                  "target_property": "length"
              },
              {
                  "narrative_system_variable": "token",
                  "target_property": "token"
              }
          ],
          "output_mapping" : [
            {
                "input_parameter": "output_profile",
                "target_property": "id"
            },
            {
                "narrative_system_variable": "workspace",
                "target_property": "ws"
            },
            {
                "narrative_system_variable": "token",
                "target_property": "auth"
            }
          ]
      }
    }
}
