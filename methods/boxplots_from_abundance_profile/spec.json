{
  "name" : "Boxplots from Abundance Profile",
  "ver" : "1.0.0",
  "authors" : [ ],
  "contact" : "help@kbase.us",
  "visble" : true,
  "categories" : ["active"],
  "widgets" : {
    "input" : null,
    "output" : "ImageView"
  },
  "parameters" : [ {
    "id" : "input_profile",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "Communities.TaxonomicMatrix", "Communities.FunctionalMatrix" ]
    }
  }, {
    "id" : "output_boxplot",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
    "text_options" : {
      "valid_ws_types" : [ "Communities.DataHandle" ],
      "is_output_name": true
    }
  }, {
    "id" : "name",
    "optional" : false,
    "advanced" : false,
    "allow_multiple" : false,
    "default_values" : [ "0" ],
    "field_type" : "checkbox",
    "checkbox_options": {
        "checked_value": 1,
        "unchecked_value": 0
    }
  } ],
  "behavior" : {
      "script-mapping" : {
          "module" : "communities_api",
          "script" : "mg-compare-boxplot-plot",
          "has_files": true,
          "input_mapping" : [
              {
                  "input_parameter": "input_profile",
                  "target_property": "input"
              },
              {
                  "input_parameter": "output_boxplot",
                  "target_property": "plot"
              },
              {
                  "input_parameter": "name",
                  "target_property": "name"
              },
              {
                  "constant_value": "1",
                  "target_property": "reference"
              },
              {
                  "narrative_system_variable": "token",
                  "target_property": "token"
              }
          ],
          "output_mapping" : [
              {
                  "input_parameter": "output_boxplot",
                  "target_property": "id"
              },
              {
                  "narrative_system_variable": "workspace",
                  "target_property": "ws"
              },
              {
                  "narrative_system_variable": "token",
                  "target_property": "auth"
              }
          ]
      }
  }
}
