    "checkbox_options":{
      "checked_value": 1,
      "unchecked_value": 0
    }
  }, {
    "id" : "full_db_model",
    "optional":false,
    "advanced":true,
    "allow_multiple":false,
    "default_values":["0"],
    "field_type" : "checkbox",
    "checkbox_options":{
      "checked_value": 1,
      "unchecked_value": 0
    }
  }, {
    "id" : "template_model",
    "optional" : true,
    "advanced" : true,
    "allow_multiple" : false,
    "default_values" : [ "" ],
    "field_type" : "text",
